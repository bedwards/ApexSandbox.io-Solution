public Map<String, String> phonebook(List<Contact> contacts) {
    Map<String, String> m = new Map<String, String>();
    for (Integer i = 0 ;  i < contacts.size(); i++) {
        Contact c = contacts.get(i);
        if (c.Phone != null) {
            m.put(c.Name, c.Phone);
        }
    }
    return m;
}